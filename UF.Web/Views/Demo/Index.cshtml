@model UF.Web.ViewModel.InventorySearchViewModel
@{
    ViewData["Title"] = "Demo Page";
}

<div  id="demo-content" class="text-center">
    @foreach (var inv in Model.Inventories) {
        <div>
            <div class="supplier-name">@inv.Supplier.Name</div>
            @foreach (var partInv in inv.Inventories) {
                <div>@partInv.Part.Number</div>
            }
        </div>
    }
</div>
